FUNCTION sign_div_unsign_hnh (denominator[15..0], numerator[15..0])
RETURNS ( quotient[15..0], remainder[15..0]);

--synthesis_resources = lut 147 
SUBDESIGN lpm_divide_4gm
( 
	denom[15..0]	:	input;
	numer[12..0]	:	input;
	quotient[12..0]	:	output;
	remain[15..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_hnh;
	gnd_wire	: WIRE;
	num_padder[2..0]	: WIRE;
	numer_tmp[15..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	gnd_wire = B"0";
	num_padder[] = ( gnd_wire, gnd_wire, gnd_wire);
	numer_tmp[] = ( num_padder[], numer[]);
	quotient[12..0] = divider.quotient[12..0];
	remain[] = divider.remainder[];
END;
